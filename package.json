{
    "private": true,
    "scripts": {
        "start": "dotnet fable watch src --run parcel ./src/index.html --open --port 8080",
        "build": "dotnet fable src --run parcel build ./src/index.html",
        "start:parcel": "parcel ./src/index.html",
        "build:parcel": "parcel build ./src/index.html",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d deploy"

    },
    "dependencies": {
        "htmlparser2": "^5.0.1",
        "react": "^16.14.0",
        "react-dom": "^16.14.0"
    },
    "devDependencies": {
        "gh-pages": "^3.1.0",
        "parcel": "2.0.0-nightly.432",
        "sass": "^1.27.0"
    }
}
